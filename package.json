{
  "name": "lens-service",
  "version": "1.0.1",
  "description": "Lens Service - Embeddable AI Customer Service Widget",
  "main": "dist/lens-service.js",
  "types": "src/index.ts",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GenAI-Agent/lens-service.git"
  },
  "bugs": {
    "url": "https://github.com/GenAI-Agent/lens-service/issues"
  },
  "homepage": "https://github.com/GenAI-Agent/lens-service#readme",
  "scripts": {
    "dev": "vite",
    "build": "esbuild src/index.ts --bundle --outfile=dist/lens-service.js --format=esm --external:@prisma/client --external:prisma --external:pg --external:openai",
    "build:vite": "vite build",
    "build:check": "tsc && vite build",
    "preview": "vite preview",
    "prepublishOnly": "npm run build",
    "server": "node server.js",
    "server:dev": "nodemon server.js",
    "db:proxy": "node database-proxy.js",
    "db:proxy:dev": "nodemon database-proxy.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev"
  },
  "keywords": [
    "lens-service",
    "customer-service",
    "ai",
    "chatbot",
    "widget",
    "openai",
    "azure"
  ],
  "author": "GenAI-Agent",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.5",
    "nodemon": "^3.0.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.11"
  },
  "dependencies": {
    "@prisma/client": "^6.16.3",
    "@types/html2canvas": "^0.5.35",
    "@types/pg": "^8.15.5",
    "axios": "^1.12.2",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "html2canvas": "^1.4.1",
    "marked": "^16.4.1",
    "pdf-parse": "^2.4.4",
    "pg": "^8.7.3",
    "pg-connection-string": "^2.9.1",
    "postgres": "^3.4.7",
    "prisma": "^6.16.3"
  },
  "peerDependencies": {
    "openai": "^4.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
